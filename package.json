{
  "name": "vue-use-api-call",
  "version": "0.1.11",
  "author": "neew-gen",
  "scripts": {
    "npm-install": "npm i",
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "release": "dotenv release-it"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vitest/ui": "^3.1.2",
    "@vue/tsconfig": "^0.7.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.2.5",
    "release-it": "^18.1.2",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.8"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neew-gen/vue-use-api-call.git"
  },
  "keywords": [
    "vue",
    "api",
    "call",
    "vue-use-api-call",
    "use-api-call",
    "query",
    "fetch"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neew-gen/vue-use-api-call/issues"
  },
  "homepage": "https://github.com/neew-gen/vue-use-api-call#readme",
  "description": "A Vue 3 composable for making easy API calls with built-in loading state."
}
